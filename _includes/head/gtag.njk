{% if GOOGLE_TAG_SITE_ID %}
	<script async src="https://www.googletagmanager.com/gtag/js?id={{ GOOGLE_TAG_SITE_ID }}"></script>
	<script>
		window.dataLayer = window.dataLayer || [];
		function gtag(){dataLayer.push(arguments);}
		gtag('js', new Date());
	
		gtag('config', '{{ GOOGLE_TAG_SITE_ID }}');
	</script>
	
{% endif  %}